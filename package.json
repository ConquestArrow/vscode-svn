{
  "name": "vscode-svn",
  "displayName": "vscode-svn",
  "description": "VSCode Subversion(svn) integration",
  "version": "0.1.0",
  "publisher": "ConquestArrow",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.svn",
        "title": "svn cmd"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typedoc": "^0.3.12",
    "typescript": "^2.0.3",
    "vsce": "^1.1.0",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32",
    "@types/xml2js": "^0.0.28",
    "@types/diff": "^0.0.31"
  },
  "dependencies": {
    "diff": "^3.1.0",
    "diff-parse": "0.0.13",
    "vscode": "^1.0.3",
    "which": "^1.2.1",
    "xml2js": "^0.4.17"
  },
  "private": true
}
