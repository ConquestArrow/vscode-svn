{
  "name": "svn",
  "displayName": "svn",
  "description": "VSCode Subversion(svn) integration",
  "version": "0.1.0",
  "publisher": "ConquestArrow",
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.svn.status",
        "title": "svn status"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "dependencies": {
    "diff": "^2.2.1",
    "diff-parse": "0.0.13",
    "which": "^1.2.1"
  },
  "private": true
}
